---
import App from '../layouts/App.astro';
import Header from '../components/header/Header-CurveHeader.astro';
import HeaderTitle from "../components/header/Header-CurveHeader-HeaderTitle.astro"
import FluidGridGallery from '../components/project/Grid-FluidGridGallery.astro';

---

<App>

	<Header idName="curveHeaderUp" className="curveHeaderUp" config={{ svgHeight: 70, isdown: false }} >
		<HeaderTitle slot="headerTitle" />
	</Header>

	<Header idName="curveHeaderDown" className="curveHeaderDown" config={{ svgHeight: 50, isdown: false }} />

	<FluidGridGallery />

</App>
